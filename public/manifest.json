{
  "manifest_version": 2,
  "name": "Addit Extension",
  "short_name": "Addit Extension",
  "version": "0.0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Addit Extension"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "all_frames": true,
    "js":      ["scripts/content.js"]
  }],
  "icons": {
    "16": "app-icon.png",
    "48": "app-icon.png",
    "128": "app-icon.png"
  },
  "permissions": [
    "storage",
    "activeTab"
  ]
}
